{
  "version": "0.2.1",
  "tasks": [
    {
      "taskLabel": "build",
      "appliesTo": "*",
      "type": "launch",
      "contextType": "build",
      "workingDirectory": "${workspaceRoot}/src",
      "command": "make",
      "args": [ "-s" ],
      "env": {
        "PATH": "${env.PATH};${env.AVR8_GNU_TOOLCHAIN}/bin"
      }
    },
    {
      "taskLabel": "rebuild",
      "appliesTo": "*",
      "type": "launch",
      "contextType": "rebuild",
      "workingDirectory": "${workspaceRoot}/src",
      "command": "make",
      "args": [ "-s", "clean", "all" ],
      "env": {
        "PATH": "${env.PATH};${env.AVR8_GNU_TOOLCHAIN}/bin"
      }
    },
    {
      "taskLabel": "clean",
      "appliesTo": "*",
      "type": "launch",
      "contextType": "clean",
      "workingDirectory": "${workspaceRoot}/src",
      "command": "make",
      "args": [ "-s", "clean" ],
      "env": {
        "PATH": "${env.PATH};${env.AVR8_GNU_TOOLCHAIN}/bin"
      }
    }
  ]
}